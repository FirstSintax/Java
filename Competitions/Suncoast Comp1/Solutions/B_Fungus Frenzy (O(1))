import java.util.Scanner;

public class BFungusFrenzy {
  
  	//Takes advantage of the fact that the problem is describing a geometric series with common ratio 2
  	//O(1)

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		double a = in.nextDouble(), b = in.nextDouble(), c = a;
		System.out.println((c >= b) ? 0 : ((c == 0) ? -1 : (int)Math.ceil(Math.log(b / c) / Math.log(2))));
	}
	
}
